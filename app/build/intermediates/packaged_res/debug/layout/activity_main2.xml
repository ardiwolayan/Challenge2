<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintSearch"
        android:layout_width="424dp"
        android:layout_height="71dp"
        android:background="@color/green"
        app:layout_constraintBottom_toTopOf="@id/constraintGopay"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <EditText
            android:id="@+id/evSearch"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/layout_in_gopay"
            android:hint="Cari layanan, makanan, &amp; tujuan"
            android:paddingLeft="50dp"
            android:paddingRight="30dp"
            android:textSize="14sp"
            app:layout_constraintLeft_toLeftOf="@id/constraintSearch"
            app:layout_constraintLeft_toRightOf="@id/icUser"
            app:layout_constraintTop_toTopOf="@id/constraintSearch" />
        <ImageView
            android:id="@+id/ivSearch"
            android:layout_width="32dp"
            android:layout_height="22dp"
            android:layout_marginLeft="-230dp"
            android:layout_marginTop="6dp"
            android:layout_marginBottom="4dp"
            android:src="@drawable/ic_search"
            app:layout_constraintBottom_toBottomOf="@id/evSearch"
            app:layout_constraintLeft_toLeftOf="@id/evSearch"
            app:layout_constraintRight_toRightOf="@id/evSearch"
            app:layout_constraintTop_toTopOf="@id/evSearch" />
        <ImageView
            android:id="@+id/icUser"
            android:layout_width="44dp"
            android:layout_height="33dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="17dp"
            android:layout_marginRight="64dp"
            android:layout_marginBottom="18dp"
            android:src="@drawable/ic_user"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="@id/constraintSearch"
            app:layout_constraintStart_toEndOf="@+id/evSearch"
            app:layout_constraintTop_toTopOf="@id/constraintSearch" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintGopay"
        android:layout_width="335dp"
        android:layout_height="84dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="16dp"
        android:layout_marginRight="50dp"
        android:background="@drawable/layout_gopay"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintSearch">
        <ImageView
            android:id="@+id/icBayar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="32dp"
            android:layout_marginBottom="44dp"
            android:src="@drawable/ic_bayar"
            app:layout_constraintBottom_toBottomOf="@id/constraintGopay"
            app:layout_constraintLeft_toRightOf="@id/constraintInGopay" />
        <ImageView
            android:id="@+id/icIsiSaldo"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="8dp"
            android:src="@drawable/ic_isi_saldo"
            app:layout_constraintBottom_toBottomOf="@id/icBayar"
            app:layout_constraintLeft_toRightOf="@id/icBayar"
            app:layout_constraintTop_toTopOf="@id/icBayar" />
        <ImageView
            android:id="@+id/icEksplor"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginBottom="8dp"
            android:src="@drawable/ic_eksplor"
            app:layout_constraintBottom_toBottomOf="@id/icIsiSaldo"
            app:layout_constraintLeft_toRightOf="@id/icIsiSaldo"
            app:layout_constraintTop_toTopOf="@id/icIsiSaldo"
            app:layout_constraintVertical_bias="0.285" />
        <TextView
            android:id="@+id/tvBayar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20dp"
            android:text="Bayar"
            android:textColor="@color/white"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@id/constraintGopay"
            app:layout_constraintStart_toStartOf="@+id/icBayar"
            app:layout_constraintTop_toBottomOf="@id/icBayar"
            app:layout_constraintVertical_bias="0.573" />
        <TextView
            android:id="@+id/tvIsiSaldo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="-5dp"
            android:text="Isi Saldo"
            android:textColor="@color/white"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@id/tvBayar"
            app:layout_constraintLeft_toRightOf="@id/tvBayar"
            app:layout_constraintTop_toBottomOf="@id/icIsiSaldo"
            app:layout_constraintVertical_bias="0.916" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="-5dp"
            android:text="Eksplor"
            android:textColor="@color/white"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@id/tvIsiSaldo"
            app:layout_constraintLeft_toRightOf="@id/tvIsiSaldo"
            app:layout_constraintTop_toBottomOf="@id/icEksplor"
            app:layout_constraintVertical_bias="1.0" />
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintInGopay"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="220dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/layout_in_gopay"
            app:layout_constraintBottom_toBottomOf="@id/constraintGopay"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintLeft_toLeftOf="@id/constraintGopay"
            app:layout_constraintRight_toRightOf="@id/constraintGopay"
            app:layout_constraintTop_toTopOf="@id/constraintGopay"
            app:layout_constraintVertical_bias="1.0">
            <ImageView
                android:id="@+id/icDompet"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:src="@drawable/ic_dompet"
                app:layout_constraintLeft_toLeftOf="@id/constraintInGopay"
                app:layout_constraintTop_toTopOf="@id/constraintInGopay" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text="Gopay"
                android:textColor="@color/black"
                android:textSize="12sp"
                android:textStyle="bold"
                app:layout_constraintLeft_toRightOf="@id/icDompet"
                app:layout_constraintTop_toTopOf="@id/icDompet" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="2dp"
                android:text="Rp109.048"
                android:textColor="@color/black"
                android:textSize="12sp"
                android:textStyle="bold"
                app:layout_constraintLeft_toLeftOf="@id/constraintInGopay"
                app:layout_constraintTop_toBottomOf="@id/icDompet" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="4dp"
                android:layout_marginTop="15dp"
                android:text="Klik &amp; cek riwayat"
                android:textColor="@color/green"
                android:textSize="12sp"
                android:textStyle="bold"
                app:layout_constraintLeft_toLeftOf="@id/constraintInGopay"
                app:layout_constraintTop_toBottomOf="@id/icDompet" />

        </androidx.constraintlayout.widget.ConstraintLayout>
     </androidx.constraintlayout.widget.ConstraintLayout>

    <GridLayout
        android:id="@+id/gl_menu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:columnCount="4"
        android:padding="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/constraintGopay">

    <LinearLayout
        android:id="@+id/menu_list1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:orientation="vertical"
        android:padding="10dp">

        <ImageButton
            android:id="@+id/icGojek"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dp"
            android:src="@drawable/ic_ojek" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="GoRide"
            android:textStyle="bold"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/menu_list2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:orientation="vertical"
        android:padding="10dp">

        <ImageButton
            android:id="@+id/ic_mobil"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dp"
            android:src="@drawable/ic_mobil" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="GoCar"
            android:textStyle="bold"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/menu_list3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:orientation="vertical"
        android:padding="10dp">

        <ImageButton
            android:id="@+id/ic_makan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dp"
            android:src="@drawable/ic_makan" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="GoFood"
            android:textStyle="bold"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/menu_list4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:orientation="vertical"
        android:padding="10dp">

        <ImageButton
            android:id="@+id/ic_gosend"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dp"
            android:src="@drawable/ic_gosend" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="GOSned"
            android:textStyle="bold"/>

    </LinearLayout>

        <LinearLayout
            android:id="@+id/menu_list5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical"
            android:padding="10dp">

            <ImageButton
                android:id="@+id/ic_gomart"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:src="@drawable/ic_gomart" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Gomart"
                android:textStyle="bold" />

        </LinearLayout>

    <LinearLayout
        android:id="@+id/menu_list6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:orientation="vertical"
        android:padding="10dp">

        <ImageButton
            android:id="@+id/ic_gopulsa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dp"
            android:src="@drawable/ic_gopulsa" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="GoPulsa"
            android:textStyle="bold"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/menu_list7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:orientation="vertical"
        android:padding="10dp">

        <ImageButton
            android:id="@+id/ic_checkin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dp"
            android:src="@drawable/ic_checkin" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Check In"
            android:textStyle="bold"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/menu_list8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:orientation="vertical"
        android:padding="10dp">

    <ImageButton
        android:id="@+id/ic_lainnya"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:src="@drawable/ic_lainya" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lainnya"
        android:textStyle="bold"/>

    </LinearLayout>
    </GridLayout>
    <!--  End List Item  -->

    <androidx.cardview.widget.CardView
        android:id="@+id/cv_wallet"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        app:cardCornerRadius="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gl_menu">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="horizontal"
            android:padding="7dp">

            <ImageButton
                android:id="@+id/icBintang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@null"
                android:src="@drawable/ic_bintang" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical"
                android:padding="5dp"
                android:paddingStart="10dp"
                android:paddingEnd="15dp">

                <TextView
                    android:id="@+id/tvXP"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingBottom="5dp"
                    android:text="193 XP lagi ada Harta Karun"
                    android:textStyle="bold" />

                <ProgressBar
                    android:id="@+id/progressBar"
                    style="@style/Widget.AppCompat.ProgressBar.Horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:indeterminate="false"
                    android:max="100"
                    android:progress="10" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <ImageButton
                        android:id="@+id/jamPasir"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:background="@null"
                        android:src="@drawable/ic_baseline_hourglass_bottom_24" />

                    <TextView
                        android:id="@+id/tvhartaKarunb"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Harta Karun bentar lagi hilang" />
                </LinearLayout>
            </LinearLayout>

            <ImageButton
                android:id="@+id/panah"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@null"
                android:src="@drawable/ic_baseline_arrow_forward_ios_24" />
        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/tv_akses_cepat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Akses Cepat"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@id/cv_wallet"
        app:layout_constraintTop_toBottomOf="@id/cv_wallet" />


    <androidx.cardview.widget.CardView
        android:id="@+id/cv_akses_cepat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="12dp"
        android:layout_margin="10dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_akses_cepat"
        android:orientation="vertical"
        app:cardElevation="6dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:id="@+id/item_akses_cepat1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:padding="5dp"
                android:layout_marginBottom="5dp"
                android:background="@drawable/underline_background"
                android:paddingStart="10dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:paddingEnd="15dp">

                <ImageView
                    android:id="@+id/ib_akses_cepat1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:src="@drawable/ic_diskon" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:text="Ada Voucher nganggur nih"
                    android:textStyle="bold" />

                <ImageButton
                    android:id="@+id/arrow1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="60dp"
                    android:background="@null"
                    android:src="@drawable/ic_baseline_arrow_forward_ios_24" />

            </LinearLayout>
            <LinearLayout
                android:id="@+id/ll_item_akses_cepat2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:padding="5dp"
                android:paddingStart="10dp"
                app:layout_constraintTop_toBottomOf="@+id/item_akses_cepat1"
                android:paddingEnd="15dp">

                <ImageView
                    android:id="@+id/ib_akses_cepat2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:src="@drawable/ic_gofood" />

                <TextView
                    android:id="@+id/tvVoucherNganggur"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:text="Ada Voucher nganggur nih"
                    android:textStyle="bold" />

                <ImageButton
                    android:id="@+id/arrow2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="60dp"
                    android:background="@null"
                    android:src="@drawable/ic_baseline_arrow_forward_ios_24" />

            </LinearLayout>


            </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/card"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        app:cardCornerRadius="15dp"
        android:elevation="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginStart="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@id/cv_akses_cepat"
        app:layout_constraintTop_toBottomOf="@+id/cv_akses_cepat"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <!-- Media -->
            <ImageView
                android:id="@+id/imgBanner"
                android:layout_width="match_parent"
                android:layout_height="194dp"
                app:srcCompat="@drawable/img_gopay_tokped"
                android:scaleType="centerCrop"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="16dp">

                <!-- Title, secondary and supporting text -->
                <TextView
                    android:id="@+id/tvPerempuan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Festival Perempuan"
                    android:textStyle="bold"
                    android:textAppearance="?attr/textAppearanceHeadline6"
                    />
                <TextView
                    android:id="@+id/tvDescription"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:text="Temukan kebutuhan cantikmu hemat pake gopay dengan cashback 50%. Klik!"
                    android:textAppearance="?attr/textAppearanceBody2"
                    android:textColor="?android:attr/textColorSecondary"
                    />

            </LinearLayout>
        </LinearLayout>
    </com.google.android.material.card.MaterialCardView>

    <LinearLayout
        android:id="@+id/ll_bottom_navigation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/card"
        app:layout_constraintBottom_toBottomOf="parent"
        >
    <LinearLayout
        android:id="@+id/ll_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:orientation="vertical"
        android:layout_marginEnd="10dp"
        >
        <ImageButton
            android:id="@+id/ic_home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_baseline_home_24"
            android:background="@null"
            android:paddingStart="20dp"
            android:paddingEnd="20dp"
            />
        <TextView
            android:id="@+id/tv_home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Beranda"
            android:textStyle="bold"
            />

    </LinearLayout>
    <LinearLayout
        android:id="@+id/ll_promo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:orientation="vertical"
        android:layout_marginStart="20dp"
        >
        <ImageButton
            android:id="@+id/ic_promo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_baseline_new_releases_24"
            android:background="@null"
            android:paddingStart="20dp"
            android:paddingEnd="20dp"
            />
        <TextView
            android:id="@+id/tv_promo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Promo"
            />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/lisPesanan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:orientation="vertical"
        android:layout_marginStart="30dp"
        >
        <ImageButton
            android:id="@+id/ic_pesanan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_baseline_wysiwyg_24"
            android:background="@null"
            android:paddingStart="20dp"
            android:paddingEnd="20dp"
            />
        <TextView
            android:id="@+id/tv_pesanan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Pesanan"
            />
    </LinearLayout>
    <LinearLayout
